@model List<Request>

<h2>History of Requests</h2>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var request in Model)
        {
            <tr>
                <td>@request.Date</td>
                <td>@request.Url</td>
            </tr>
        }
    </tbody>
</table>
